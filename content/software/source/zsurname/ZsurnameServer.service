[Unit]
Description=zouhuigang zsurname server
Documentation=https://github.com/zouhuigang
After=network.target

[Service]
Type=simple
WorkingDirectory=/usr/local/software/zsurname
ExecStart=/usr/local/software/zsurname/ZsurnameServer
Restart=on-failure
PrivateTmp=true

[Install]
WantedBy=multi-user.target
